new_name=['santa Maria','Hello World','Marry christmas','tHank You']
new=[]
sub_list=[]
for i in new_name:
    new.append(i.split(' ')[0])
    new.append(i.split(' ')[1])
for i in new:
    if i[0].isupper():
        sub_list.append(i)
print("The Sub list Store the first upper case letter's words : ",sub_list)
